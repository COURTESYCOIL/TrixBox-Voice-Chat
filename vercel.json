{
  "buildCommand": "echo 'No build step required'",
  "outputDirectory": "index.html",
  "framework": "static",
  "public": true,
  "env": {},
  "regions": ["iad1"],
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}
